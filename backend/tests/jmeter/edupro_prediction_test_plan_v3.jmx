      <BeanShellPreProcessor guiclass="BeanShellPreProcessorGui" testclass="BeanShellPreProcessor" testname="Data Validation" enabled="true">
        <stringProp name="filename"></stringProp>
        <stringProp name="parameters"></stringProp>
        <boolProp name="resetInterpreter">false</boolProp>
        <stringProp name="script">
// Validate siswa_id
String siswaId = vars.get("siswa_id");
if (siswaId != null) {
    try {
        int id = Integer.parseInt(siswaId);
        if (id < 1) {
            vars.put("skip_request", "true");
            log.info("Invalid siswa_id: " + siswaId);
        }
    } catch (Exception e) {
        vars.put("skip_request", "true");
        log.info("Invalid siswa_id format: " + siswaId);
    }
}

// Validate semester
String semester = vars.get("semester");
if (semester != null) {
    if (!semester.equals("Ganjil") && !semester.equals("Genap")) {
        vars.put("skip_request", "true");
        log.info("Invalid semester: " + semester);
    }
}

// Validate tahun_ajaran
String tahunAjaran = vars.get("tahun_ajaran");
if (tahunAjaran != null) {
    if (!tahunAjaran.matches("\\d{4}/\\d{4}")) {
        vars.put("skip_request", "true");
        log.info("Invalid tahun_ajaran format: " + tahunAjaran);
    }
}
        </stringProp>
      </BeanShellPreProcessor>
